---
interface Props {
  title: string;
}

const { title } = Astro.props;
var year = new Date().getFullYear();
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <meta
      name="description"
      content="Automotive parts quality engineer, BI solutions and software developer mangager at ISRINGHAUSEN. BI solutions "
    />
    <meta name="title" content="Guillermo Dean" />
    <meta name="url" content="http://guillermodean.com/" />
    <meta
      name="keywords"
      content="Engineer, Guillermo, Dean, Software, Developer, BI, Pamplona, Spain"
    />
    <meta
      property="og:image"
      content="https://guillermodean.com/img/12082017-DSC00304.jpg"
    />
    <meta name="author" content="Guillermo Dean" />

    <!-- Fonts -->
	<link rel="icon" type="image/png" href="https://icon.horse/icon/gd.com">
    <link
      href="https://fonts.googleapis.com/css2?family=B612:wght@700&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Hind+Siliguri:wght@500&display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU"
      crossorigin="anonymous"
    />

    <!-- Favicons -->
    <link
      rel="apple-touch-icon"
      href="/docs/5.1/assets/img/favicons/apple-touch-icon.png"
      sizes="180x180"
    />
    <link
      rel="icon"
      href="/docs/5.1/assets/img/favicons/favicon-32x32.png"
      sizes="32x32"
      type="image/png"
    />
    <link
      rel="icon"
      href="/docs/5.1/assets/img/favicons/favicon-16x16.png"
      sizes="16x16"
      type="image/png"
    />
    <link rel="manifest" href="/docs/5.1/assets/img/favicons/manifest.json" />
    <link
      rel="mask-icon"
      href="/docs/5.1/assets/img/favicons/safari-pinned-tab.svg"
      color="#7952b3"
    />

    <!-- scripts  -->

    <script
      src="https://code.jquery.com/jquery-3.6.0.js"
      integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk="
      crossorigin="anonymous"></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
      integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
      crossorigin="anonymous"></script>
    <script
      src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
      integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
      crossorigin="anonymous"></script>

    <title>GDWEB</title>
  </head>
  <header>
    <nav
      class="navbar header navbar-expand-lg fixed-top justify-content-sm-between"
    >
      <a class="navbar-brand ml-6" href="./">Home</a>
      <button
        class="navbar-toggler m-2"
        type="button"
        data-toggle="collapse"
        data-target="#navbarNav"
        aria-controls="navbarNav"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"
          ><svg
            xmlns="http://www.w3.org/2000/svg"
            width="30"
            height="30"
            fill="currentColor"
            class="bi bi-list"
            viewBox="0 0 16 16"
          >
            <path
              fill-rule="evenodd"
              d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z"
            ></path>
          </svg></span
        >
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link" href="./#Work-experiences">Work experiences</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="./#Projects">Projects</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="./#sideline">With passion</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="./#Cuadrilla">Family and Friends</a>
          </li>
          <!-- navlink To BFMs -->
          <!-- <li>
                    <a class="nav-link" href="/BFM" >BFMs</a>
                </li> -->
        </ul>
      </div>
    </nav>
  </header>

  <body>
    <slot />
    <!-- <div class="container-fluid"> -->
    <!-- include partials -->
    <!-- <%- include('partials/header.ejs') %> -->

    <!-- include main -->
    <!-- <%- include('main.ejs') %> -->
    <!-- </div> -->
  </body>
  <footer>
    <div class="container-fluid m-lg-auto p-lg-5">
      <p class="float-right">
        <a href="#">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            width="46"
            height="46"
            fill="#0161C6"
            class="bi bi-arrow-up-circle-fill"
            viewBox="0 0 16 16"
          >
            <path
              d="M16 8A8 8 0 1 0 0 8a8 8 0 0 0 16 0zm-7.5 3.5a.5.5 0 0 1-1 0V5.707L5.354 7.854a.5.5 0 1 1-.708-.708l3-3a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1-.708.708L8.5 5.707V11.5z"
            ></path>
          </svg>
        </a>
      </p>
      <p class="lead">&copy; {year} Guillermo Dean</p>
    </div>
  </footer>

  <!-- <script>
  
  document.getElementById("currentYear").innerHTML = year;
</script> -->

  <!-- <html lang="en"></html> -->
  <style is:global>
    :root {
      --accent: 136, 58, 234;
      --accent-light: 224, 204, 250;
      --accent-dark: 49, 10, 101;
      --accent-gradient: linear-gradient(
        45deg,
        rgb(var(--accent)),
        rgb(var(--accent-light)) 30%,
        white 60%
      );
    }
    body,
    html {
      background-color: whitesmoke;
      color: #666666;
      font-family: "Hind Siliguri";
      font-size: clamp(1.125rem, 2vw, 1.25rem)
      top: 0px;
      bottom: 0px;
      left: 0px;
      right: 0px;
      width: 100%;
      scroll-behavior: smooth;
      /* overflow-y: hidden;  */
    }


    .navbar {
      width: 100%;
    }

    .header {
      background-image: linear-gradient(to bottom, #444444, rgba(255, 0, 0, 0));
      color: whitesmoke;
      padding-left: 1rem;
      padding-right: 1rem;
      width: 100%;
    }
    .header a {
      color: whitesmoke;
    }

    .logo {
      max-width: 100%;
      height: auto;
    }

    .navbar-toggler {
      background-color: rgba(255, 0, 0, 0);
      /* #e0ca3c; */
      color: whitesmoke;
    }
	.footer {
    padding-top: 0.5rem;
    max-width: 100%;
    padding-right: 1rem;
    padding-right: 1rem;
  }
  .float-right {
    color: #0161c6;
    position: fixed;
    bottom: 20px;
    right: 20px;
    padding: 10px 20px;
  }
  </style>

</html>
